$('#dashboard-container').html("<%= escape_javascript(render(partial: 'index', locals: { campaigns: @campaigns })) %>")

$('#campaigns-index').addClass("animated fadeIn");

setTimeout(function() {
  $('#campaigns-index').removeClass('animated fadeIn');
}, 1000);

// Initiate Paginator

$(function() {
  $(".pagination a").on("click", function() {
    $(".pagination").html("Loading the vials..");
    $.getScript(this.href);
    return false;
  });
});